project 'Clima.xcodeproj'

platform :ios, '9.0'

# do is open block and end is close block: Ruby syntax.
target 'Clima' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Clima, found in cocoapods.org

    pod 'SwiftyJSON'
    pod 'Alamofire'
    pod 'SVProgressHUD'

end
